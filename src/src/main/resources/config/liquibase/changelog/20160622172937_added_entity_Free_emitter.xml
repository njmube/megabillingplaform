<?xml version="1.0" encoding="utf-8"?>
<databaseChangeLog
    xmlns="http://www.liquibase.org/xml/ns/dbchangelog"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://www.liquibase.org/xml/ns/dbchangelog http://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-3.4.xsd">

    <property name="now" value="now()" dbms="mysql,h2"/>
    <property name="now" value="current_timestamp" dbms="postgresql"/>
    <property name="now" value="sysdate" dbms="oracle"/>

    <property name="autoIncrement" value="true" dbms="mysql,h2,postgresql,oracle"/>

    <property name="floatType" value="float4" dbms="postgresql, h2"/>
    <property name="floatType" value="float" dbms="mysql, oracle"/>

    <!--
        Added the entity Free_emitter.
    -->
    <changeSet id="20160622172937-1" author="jhipster">
        <createTable tableName="free_emitter">
            <column name="id" type="bigint" autoIncrement="${autoIncrement}">
                <constraints primaryKey="true" nullable="false"/>
            </column>
            <column name="rfc" type="varchar(13)">
                <constraints nullable="false" />
            </column>
            
            <column name="name" type="varchar(50)">
                <constraints nullable="false" />
            </column>
            
            <column name="email" type="varchar(254)">
                <constraints nullable="false" />
            </column>
            
            <column name="path_certificate" type="blob">
                <constraints nullable="false" />
            </column>
            <column name="path_certificate_content_type" type="varchar(255)">
                <constraints nullable="false" />
            </column>
            
            <column name="private_key" type="blob">
                <constraints nullable="false" />
            </column>
            <column name="private_key_content_type" type="varchar(255)">
                <constraints nullable="false" />
            </column>
            
            <column name="reference" type="varchar(255)">
                <constraints nullable="true" />
            </column>
            
            <column name="num_in" type="varchar(255)">
                <constraints nullable="true" />
            </column>
            
            <column name="num_out" type="varchar(255)">
                <constraints nullable="true" />
            </column>
            
            <column name="street" type="varchar(255)">
                <constraints nullable="false" />
            </column>
            
            <column name="tax_regime_id" type="bigint">
                <constraints nullable="true" />
            </column>

            <column name="c_country_id" type="bigint">
                <constraints nullable="true" />
            </column>

            <column name="c_state_id" type="bigint">
                <constraints nullable="true" />
            </column>

            <column name="c_municipality_id" type="bigint">
                <constraints nullable="true" />
            </column>

            <column name="c_location_id" type="bigint">
                <constraints nullable="true" />
            </column>

            <column name="c_colony_id" type="bigint">
                <constraints nullable="true" />
            </column>

            <column name="c_zip_code_id" type="bigint">
                <constraints nullable="true" />
            </column>

            <!-- jhipster-needle-liquibase-add-column - Jhipster will add columns here, do not remove-->
        </createTable>
        
    </changeSet>
</databaseChangeLog>
