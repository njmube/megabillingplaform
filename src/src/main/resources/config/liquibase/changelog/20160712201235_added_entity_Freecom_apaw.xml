<?xml version="1.0" encoding="utf-8"?>
<databaseChangeLog
    xmlns="http://www.liquibase.org/xml/ns/dbchangelog"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://www.liquibase.org/xml/ns/dbchangelog http://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-3.4.xsd">

    <property name="now" value="now()" dbms="mysql,h2"/>
    <property name="now" value="current_timestamp" dbms="postgresql"/>
    <property name="now" value="sysdate" dbms="oracle"/>

    <property name="autoIncrement" value="true" dbms="mysql,h2,postgresql,oracle"/>

    <property name="floatType" value="float4" dbms="postgresql, h2"/>
    <property name="floatType" value="float" dbms="mysql, oracle"/>

    <!--
        Added the entity Freecom_apaw.
    -->
    <changeSet id="20160712201235" author="jhipster">
        <createTable tableName="freecom_apaw">
            <column name="id" type="bigint" autoIncrement="${autoIncrement}">
                <constraints primaryKey="true" nullable="false"/>
            </column>
            <column name="version" type="varchar(255)">
                <constraints nullable="false" />
            </column>
            
            <column name="others_well_type" type="varchar(255)">
                <constraints nullable="true" />
            </column>
            
            <column name="others_acquired_title" type="varchar(255)">
                <constraints nullable="true" />
            </column>
            
            <column name="subtotal" type="decimal(10,2)">
                <constraints nullable="true" />
            </column>
            
            <column name="iva" type="decimal(10,2)">
                <constraints nullable="true" />
            </column>
            
            <column name="date_acquisition" type="timestamp">
                <constraints nullable="false" />
            </column>
            
            <column name="well_type_id" type="bigint"/>
            <column name="acquired_title_id" type="bigint"/>
            <column name="features_work_piece_id" type="bigint"/>
            <column name="free_cfdi_id" type="bigint">
                <constraints unique="true"/>
            </column>
            <!-- jhipster-needle-liquibase-add-column - Jhipster will add columns here, do not remove-->
        </createTable>
        <dropDefaultValue tableName="freecom_apaw" columnName="date_acquisition" columnDataType="datetime"/>
        
        <addForeignKeyConstraint baseColumnNames="well_type_id"
                                 baseTableName="freecom_apaw"
                                 constraintName="fk_freecom_apaw_well_type_id"
                                 referencedColumnNames="id"
                                 referencedTableName="well_type"/>

        <addForeignKeyConstraint baseColumnNames="acquired_title_id"
                                 baseTableName="freecom_apaw"
                                 constraintName="fk_freecom_apaw_acquired_title_id"
                                 referencedColumnNames="id"
                                 referencedTableName="acquired_title"/>

        <addForeignKeyConstraint baseColumnNames="features_work_piece_id"
                                 baseTableName="freecom_apaw"
                                 constraintName="fk_freecom_apaw_features_work_piece_id"
                                 referencedColumnNames="id"
                                 referencedTableName="features_work_piece"/>

        <addForeignKeyConstraint baseColumnNames="free_cfdi_id"
                                 baseTableName="freecom_apaw"
                                 constraintName="fk_freecom_apaw_free_cfdi_id"
                                 referencedColumnNames="id"
                                 referencedTableName="free_cfdi"/>

    </changeSet>
</databaseChangeLog>
