<?xml version="1.0" encoding="utf-8"?>
<databaseChangeLog
    xmlns="http://www.liquibase.org/xml/ns/dbchangelog"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://www.liquibase.org/xml/ns/dbchangelog http://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-3.4.xsd">

    <property name="now" value="now()" dbms="mysql,h2"/>
    <property name="now" value="current_timestamp" dbms="postgresql"/>
    <property name="now" value="sysdate" dbms="oracle"/>

    <property name="autoIncrement" value="true" dbms="mysql,h2,postgresql,oracle"/>

    <property name="floatType" value="float4" dbms="postgresql, h2"/>
    <property name="floatType" value="float" dbms="mysql, oracle"/>

    <!--
        Added the entity Com_educational_institutions.
    -->
    <changeSet id="20160712222008-1" author="jhipster">
        <createTable tableName="com_educational_institutions">
            <column name="id" type="bigint" autoIncrement="${autoIncrement}">
                <constraints primaryKey="true" nullable="false"/>
            </column>
            <column name="version" type="varchar(255)">
                <constraints nullable="false" />
            </column>

            <column name="name_student" type="varchar(255)">
                <constraints nullable="false" />
            </column>

            <column name="curp" type="varchar(255)">
                <constraints nullable="false" />
            </column>

            <column name="autrvoe" type="varchar(255)">
                <constraints nullable="false" />
            </column>

            <column name="rfcpayment" type="varchar(255)">
                <constraints nullable="true" />
            </column>
                        <column name="cfdi_id" type="bigint">
                <constraints unique="true" nullable="true" />
            </column>
            <column name="c_school_level_id" type="bigint">
                <constraints nullable="true" />
            </column>

            <!-- jhipster-needle-liquibase-add-column - Jhipster will add columns here, do not remove-->
        </createTable>
        <addForeignKeyConstraint baseColumnNames="cfdi_id"
                                 baseTableName="com_educational_institutions"
                                 constraintName="fk_com_educational_institutions_cfdi_id"
                                 referencedColumnNames="id"
                                 referencedTableName="cfdi"/>

        <addForeignKeyConstraint baseColumnNames="c_school_level_id"
                                 baseTableName="com_educational_institutions"
                                 constraintName="fk_com_educational_institutions_c_school_level_id"
                                 referencedColumnNames="id"
                                 referencedTableName="c_school_level"/>
    </changeSet>
</databaseChangeLog>
