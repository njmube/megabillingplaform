<?xml version="1.0" encoding="utf-8"?>
<databaseChangeLog
    xmlns="http://www.liquibase.org/xml/ns/dbchangelog"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://www.liquibase.org/xml/ns/dbchangelog http://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-3.4.xsd">
    <!--
        Added the constraints for entity C_location.
    -->
    <changeSet id="20160501144335-2" author="jhipster">
        
        <addForeignKeyConstraint baseColumnNames="c_municipality_id"
                                 baseTableName="c_location"
                                 constraintName="fk_c_location_c_municipality_id"
                                 referencedColumnNames="id"
                                 referencedTableName="c_municipality"/>

        <addForeignKeyConstraint baseColumnNames="c_zip_code_id"
                                 baseTableName="c_location"
                                 constraintName="fk_c_location_c_zip_code_id"
                                 referencedColumnNames="id"
                                 referencedTableName="c_zip_code"/>

    </changeSet>
</databaseChangeLog>
